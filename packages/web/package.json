{
  "name": "web",
  "version": "2.0.2",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "yarn prisma migrate dev && next dev",
    "ci:server": "yarn prisma generate && yarn prisma migrate deploy && next dev",
    "build": "yarn prisma generate && yarn prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc",
    "build-emails": "cp -r ./emails/assets public && yarn mailing server build",
    "dev-emails": "yarn mailing",
    "dev-emails:local-assets": "npx mailing server scaffold && ln -sf ../../emails/assets ./.mailing/public && LOCAL_ASSETS=true yarn mailing preview"
  },
  "dependencies": {
    "@catloafsoft/mailing-core": "^2.0.2",
    "@docsearch/react": "^3.9.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "3.1.0",
    "@next/font": "^14.2.15",
    "@next/mdx": "^15.4.7",
    "@prisma/client": "^6.14.0",
    "@vercel/analytics": "^1.5.0",
    "classnames": "^2.5.1",
    "mailing-core": "^1.1.0",
    "next": "15.4.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-intersection-observer": "^9.16.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@jest/globals": "30.0.5",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "13.5.11",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0",
    "mailing": "^1.1.0",
    "postcss": "^8.5.6",
    "prisma": "6.14.0",
    "tailwindcss": "^3.4.17"
  }
}
