{
  "name": "web",
  "version": "2.0.8",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "pnpm exec prisma migrate dev && pnpm exec next dev",
    "ci:server": "pnpm exec prisma generate && pnpm exec prisma migrate deploy && pnpm exec next dev",
    "build": "pnpm exec prisma generate && pnpm exec prisma migrate deploy && pnpm exec next build",
    "start": "pnpm exec next start",
    "lint": "pnpm exec next lint",
    "type-check": "pnpm exec tsc",
    "build-emails": "cp -r ./emails/assets public && pnpm exec mailing server build",
    "dev-emails": "pnpm exec mailing",
    "dev-emails:local-assets": "pnpm exec mailing server scaffold && ln -sf ../../emails/assets ./.mailing/public && LOCAL_ASSETS=true pnpm exec mailing preview"
  },
  "dependencies": {
    "@catloafsoft/mailing-core": "^2.0.2",
    "@docsearch/react": "^4.0.0",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "3.1.1",
    "@next/font": "^14.2.15",
    "@next/mdx": "^15.5.3",
    "@prisma/client": "^6.16.1",
    "@vercel/analytics": "^1.5.0",
    "classnames": "^2.5.1",
    "mailing-core": "^1.1.0",
    "next": "15.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-intersection-observer": "^9.16.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@jest/globals": "30.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.6",
    "jest": "^30.1.3",
    "jest-mock-extended": "^4.0.0",
    "mailing": "^1.1.0",
    "postcss": "^8.5.6",
    "prisma": "6.16.1",
    "tailwindcss": "^3.4.17"
  }
}
