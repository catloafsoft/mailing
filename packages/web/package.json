{
  "name": "web",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "yarn prisma migrate dev && next dev",
    "ci:server": "yarn prisma generate && yarn prisma migrate deploy && next dev",
    "build": "yarn prisma generate && yarn prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc",
    "build-emails": "cp -r ./emails/assets public && yarn mailing server build",
    "dev-emails": "yarn mailing",
    "dev-emails:local-assets": "npx mailing server scaffold && ln -sf ../../emails/assets ./.mailing/public && LOCAL_ASSETS=true yarn mailing preview"
  },
  "dependencies": {
    "@docsearch/react": "^3.9.0",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "3.1.0",
    "@next/font": "^13.5.11",
    "@next/mdx": "^13.5.11",
    "@prisma/client": "^6.6.0",
    "@vercel/analytics": "^1.5.0",
    "classnames": "^2.5.1",
    "mailing-core": "^1.0.1",
    "next": "14.2.28",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-intersection-observer": "^9.16.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.3.1",
    "jest-mock-extended": "^2.0.9",
    "mailing": "^1.0.1",
    "postcss": "^8.5.3",
    "prisma": "6.6.0",
    "tailwindcss": "^3.4.17"
  }
}
